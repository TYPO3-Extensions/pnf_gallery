<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<T3DataStructure>
	<meta>
       <langDisable>1</langDisable>
	</meta>
	<sheets>
		<general>
			<ROOT>
				<TCEforms>
					<sheetTitle>LLL:EXT:pnf_gallery/locallang_flexform_pi1.xml:general</sheetTitle>
				</TCEforms>
				<type>array</type>
				<el>
					<source>
						<TCEforms>
							<onChange>reload</onChange>
							<label>LLL:EXT:pnf_gallery/locallang_flexform_pi1.xml:sources</label>
							<config>	
								<type>select</type>
								<itemsProcFunc>tx_pnfgallery_flex_itemsProcFunc->user_insertSources</itemsProcFunc>
								<maxitems>1</maxitems>
								<size>1</size>
							</config>
						</TCEforms>
					</source>
					<mode>
						<TCEforms>
							<onChange>reload</onChange>
							<label>LLL:EXT:pnf_gallery/locallang_flexform_pi1.xml:mode</label>
							<config>
								<type>select</type>
								<itemsProcFunc>tx_pnfgallery_flex_itemsProcFunc->user_insertModes</itemsProcFunc>
								<maxitems>1</maxitems>
								<size>1</size>
							</config>
						</TCEforms>
					</mode>
					<!-- ###ADDITIONAL FLEX DATA MODE### -->
				</el>
			</ROOT>
		</general>
		<design>
			<ROOT>
				<TCEforms>
					<sheetTitle>LLL:EXT:pnf_gallery/locallang_flexform_pi1.xml:design</sheetTitle>
				</TCEforms>
				<type>array</type>
				<el>
					<width>
						<TCEforms>
							<label>LLL:EXT:pnf_gallery/locallang_flexform_pi1.xml:width</label>
							<config>
								<type>input</type>
								<size>3</size>
								<default></default>
							</config>
						</TCEforms>
					</width>
					<height>
						<TCEforms>
							<label>LLL:EXT:pnf_gallery/locallang_flexform_pi1.xml:height</label>
							<config>
								<type>input</type>
								<size>3</size>
								<default></default>
							</config>
						</TCEforms>
					</height>
					<theme>
						<TCEforms>
							<onChange>reload</onChange>
							<label>LLL:EXT:pnf_gallery/locallang_flexform_pi1.xml:theme</label>
							<config>
								<type>select</type>
								<itemsProcFunc>tx_pnfgallery_flex_itemsProcFunc->user_insertTheme</itemsProcFunc>
								<maxitems>1</maxitems>
								<size>1</size>
							</config>
						</TCEforms>
					</theme>
					<!-- ###ADDITIONAL FLEX DATA THEME### -->
				</el>
			</ROOT>
		</design>
	</sheets>
</T3DataStructure>